<script>
    var com = new Vue({
        el: "#firmregister",
        data: {
            firmregisterInformation: {
                account: "",
                password: "",
                lastName: "",
                firstName: "",
                phone: "",
                roleName: "Firm",
                firms: {
                    name: "",
                    taxId: ""
                }
            }
        },
        methods: {
            addRegister: function () {
                let self = this;
                axios.post("/auth/register", self.firmregisterInformation)
                    .then(
                        function (res) {
                            if (res.status == 200) {
                                Swal.fire({
                                    position: 'center',
                                    icon: 'success',
                                    title: '恭喜註冊成功',
                                    showConfirmButton: false,
                                    timer: 1500,
                                    willClose: (toast) => {
                                        axios.post("/auth/login", self.firmregisterInformation).then(function (res) { window.location.href = "/backstage/home"; }).catch();
                                    }
                                });
                                //alert("恭喜註冊成功，請再次登入，並至您的信箱接收帳號激活驗證信");
                                //axios.post("/auth/login", self.firmregisterInformation).then(function (res) { window.location.href = "/backstage/home"; }).catch();
                            };
                        }
                    )
                    .catch(function (error) {
                        if (error.status == 404) {
                            alert(error.data);
                        }
                        if (error.status == 422) {
                            for (var i of Object.keys(error.data.errors)) {
                                alert(error.data.errors[i][0]);
                            }
                        }
                    });
            },
            checkEmail: function () {
                let self = this;
                axios.post("/auth/checkAccount?account=" + self.firmregisterInformation.account)
                    .then(function (res) {
                        if (res.status == 200) {
                            if (res.data == "帳號已存在") {
                                alertWrong(res.data);
                            }
                            else {
                                alertSuccess(res.data);
                            }
                        }
                    });
            }
        }
    });
</script>


<script>
    function alertSuccess(inp) {
        Swal.fire({
            position: 'center',
            icon: 'success',
            title: inp,
            showConfirmButton: false,
            timer: 1500,
        });
    }

    function alertWrong(inp) {
        Swal.fire({
            icon: 'error',
            title: 'Oops...',
            text: inp,
        })
    }
</script>