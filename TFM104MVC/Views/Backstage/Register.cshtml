<body>

    <!--導覽列-->
    <header>
        <div class="container">
            <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-light">
                <div class="container">
                    <div style="margin-right: 3em;">
                        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="/pic/newest_logo.png" style="width:150px;"></a>
                    </div>
                    <div class="navbar-brand" style="">
                        <a class="navbar-brand" asp-area="" asp-controller="Backstage" asp-action="Login" style="color:#26bec9;font-size:16px;">
